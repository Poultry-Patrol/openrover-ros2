<?xml version="1.0" ?>
<robot name="2WD_rover_robotics" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find openrover_demo)/urdf/common_properties.xacro"/>
  <xacro:include filename="$(find openrover_demo)/urdf/2WD_Rover.gazebo.xacro"/>

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link" />
    <origin xyz="0 0 0.12" rpy="0 0 0"/>
  </joint>

  <link name="base_link">
    <visual>
      <origin xyz="-0.142 0 0.055" rpy="0 0 1.57"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/2WD_OpenRover.stl" scale="1 1 1"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="-0.14 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.42 0.25 0.12"/>
      </geometry>
    </collision>
    
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.3729096e+00"/>
      <inertia ixx="8.7002718e-03" ixy="-4.7576583e-05" ixz="1.1160499e-04"
               iyy="8.6195418e-03" iyz="-3.5422299e-06"
               izz="1.4612727e-02" />
    </inertial>
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_link"/>
    <origin xyz="0 0.179 0" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_left_link">
    <visual>
      <origin xyz="0 0 0" rpy="-1.57 0 0"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/left_wheel.stl" scale="1 1 1"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.07" radius="0.12"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
      </inertial>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <origin xyz="0 -0.179 0" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_right_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/right_wheel.stl" scale="1 1 1"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.07" radius="0.12"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
    </inertial>
  </link>





  <joint name="sheetmetal_joint" type="fixed">
    <parent link="base_link"/>
    <child link="sheetmetal_link"/>
    <origin xyz="-0.35 0 0" rpy="0 0 0"/>
  </joint>

  <link name="sheetmetal_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 -1.57"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/sheetmetal_part.stl" scale="1 1 1"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="-0.05 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.065 0.3 0.008"/>
      </geometry>
    </collision>
  </link>



  <joint name="plate_link_joint" type="fixed">
    <parent link="base_link"/>
    <child link="plate_link"/>
    <origin xyz="-0.1365 0 0.127" rpy="0 0 0"/>
  </joint>

  <link name="plate_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.381 0.254 0.005"/>
      </geometry>
      <material name="light_black"/>
    </visual>
  </link>



  <joint name="imu_link_joint" type="fixed">
    <parent link="plate_link"/>
    <child link="imu_link"/>
    <origin xyz="0.1365 0 0.0026" rpy="0 0 0"/>
  </joint>

  <link name="imu_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.002"/>
      </geometry>
      <material name="light_black"/>
    </visual>
  </link>



  <joint name="cpu_link_joint" type="fixed">
    <parent link="plate_link"/>
    <child link="cpu_link"/>
    <origin xyz="-0.0032 0 0.0275" rpy="0 0 0"/>
  </joint>

  <link name="cpu_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.111 0.117 0.05"/>
      </geometry>
      <material name="light_black"/>
    </visual>
  </link>



  <joint name="lidar_link_joint" type="fixed">
    <parent link="cpu_link"/>
    <child link="lidar_link"/>
    <origin xyz="0 0 0.0488" rpy="0 0 0"/>
  </joint>

  <link name="lidar_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.0476" radius="0.0381"/>
      </geometry>
      <material name="light_black"/>
    </visual>
  </link>



  <joint name="caster_back_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="caster_back_right_link"/>
    <origin xyz="-0.36 -0.17 -0.01" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="caster_back_right_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/right_caster_wheel.stl" scale="1 1 1"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="-0.028 0 -0.065" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.035" radius="0.048"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.005" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0"
               iyy="0.001" iyz="0.0"
               izz="0.001" />
    </inertial>
  </link>





  <joint name="caster_back_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="caster_back_left_link"/>
    <origin xyz="-0.36 0.17 -0.01" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="caster_back_left_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/left_caster_wheel.stl" scale="1 1 1"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="-0.028 0 -0.065" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.035" radius="0.048"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.005" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0"
               iyy="0.001" iyz="0.0"
               izz="0.001" />
    </inertial>
  </link>


</robot>
